[test5:
		(?id ?var updateOtherVar(?prev,?next,?cond,?pp,?currVar))
	
		(?varList rdf:type pa:varList)
		(?varList rdf:item ?currVar)
		
->
		(pa:varStatus 1 updateOtherVar(?prev,?next,?cond,?pp,?currVar))
